{
  "name": "splunk-example-ta",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "webpack serve",
    "build": "cross-env NODE_ENV=production webpack --bail",
    "build:watch": "webpack --watch",
    "lint": "eslint && npm run format:verify",
    "lint:fix": "eslint --fix && npm run format",
    "format": "prettier \"src/**/*.(js|jsx|ts|tsx|css)\" --write",
    "format:verify": "prettier \"src/**/*.(js|jsx|ts|tsx|css)\" --list-different"
  },
  "dependencies": {
    "@splunk/react-ui": "^4.39.0",
    "es-toolkit": "^1.27.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.15.0",
    "@splunk/babel-preset": "^4.0.0",
    "@splunk/webpack-configs": "^7.0.2",
    "@types/node": "^22.10.0",
    "@types/react": "^16.14.62",
    "@types/react-dom": "^16.9.24",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^15.12.0",
    "prettier": "^3.4.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.16.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
