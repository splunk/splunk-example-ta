    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
